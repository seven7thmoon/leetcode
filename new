/*
 * @param {number[]} height
 * @return {number}
 */
 var maxArea = function(height) {
    let left = 0;
    let right = height.length - 1;
    let area = 0;

    while(left < right){
        let tempHeight = Math.min(height[left], height[right]);
        let tempArea = (right - left) * tempHeight;
        
        area = Math.max(area, tempArea);

        if(height[left] < height[right]){
            left += 1;
        }else{
            right -= 1;
        }
    }
    return area;
};